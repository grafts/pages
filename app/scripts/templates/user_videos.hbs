{{#if drafts}}
<div class="grid-r">
	<div class="grid-c-7-10">
		<div class="wrapper">
			<hr/>
			<h4>Published Videos</h4>
			<ul class="published list">
				{{#each published}}
				<li>
					<a href="/video/{{this.objectId}}">
						<span>
							<p class="title">{{this.title}}</p>
							<p class="subtitle">{{this.subtitle}}</p>
						</span>
					</a>
				</li>
				{{/each}}
			</ul>
		</div>
	</div>
	<div class="grid-c-3-10">
		<div class="wrapper">
			<hr/>
			<h4>Drafts</h4>
			<ul class="drafts list">
				{{#each drafts}}
				<li>
					<a href="/video/{{this.objectId}}">
						<span>
							<p class="title">{{this.title}}</p>
						</span>
					</a>
				</li>
				{{/each}}
			</ul>
		</div>
	</div>
</div>
{{else}}
<hr/>
<h4>Videos</h4>
<ul class="published list">
	{{#each published}}
	<li>
		<a href="/video/{{this.objectId}}">
{{!-- 			<div class="thumb">
				<img class="thumb" src="http://img.youtube.com/vi/{{this.objectId}}/1.jpg" alt="class_cover">
			</div>
 --}}			<span>
				<p class="title">{{this.title}}</p>
				<p class="subtitle">{{this.subtitle}}</p>
			</span>
		</a>
	</li>
	{{/each}}
</ul>
{{/if}}